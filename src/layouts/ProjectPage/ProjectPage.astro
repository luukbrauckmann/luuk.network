---
import type { ComponentProps } from 'astro/types';
import type { Page } from '@utils/page';
import { Base } from '@layouts';

type Props = {
  page: Page<'ProjectPageRecord'>;
} & ComponentProps<typeof Base>;

const { page, ...props } = Astro.props;
---

<Base {...props}>
    <h1>{ page.title }</h1>
</Base>
